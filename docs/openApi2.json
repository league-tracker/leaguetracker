    "/api/v1/seasons": {
      "post": {
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "description": "season creation options",
            "in": "body",
            "name": "body",
            "schema": {
              "properties": {
                "playersOptions": {
                  "items": {
                    "$ref": "#/definitions/PlayerOptions"
                  },
                  "type": "array"
                },
                "seasonName": {
                  "type": "string"
                }
              },
              "required": [
                "seasonName",
                "playersOptions"
              ],
              "type": "object"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Season created",
            "schema": {
              "$ref": "#/definitions/Season"
            }
          },
          "400": {
            "description": "Bad input parameter"
          }
        },
        "tags": [
          "users"
        ],
        "summary": "Create a new season"
      },
      "put": {
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "description": "season update options",
            "in": "body",
            "name": "body",
            "required": true,
            "schema": {
              "properties": {
                "games": {
                  "items": {
                    "$ref": "#/definitions/PastGame"
                  },
                  "type": "array"
                },
                "season": {
                  "$ref": "#/definitions/Season"
                }
              },
              "required": [
                "season",
                "games"
              ],
              "type": "object"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Season updated",
            "schema": {
              "properties": {
                "deltas": {
                  "items": {
                    "type": "integer"
                  },
                  "type": "array"
                },
                "season": {
                  "properties": {
                    "players": {
                      "items": {
                        "$ref": "#/definitions/Player"
                      },
                      "type": "array"
                    },
                    "seasonName": {
                      "type": "string"
                    }
                  },
                  "type": "object"
                }
              },
              "type": "object"
            }
          },
          "400": {
            "description": "Bad input parameter"
          }
        },
        "tags": [
          "users"
        ],
        "summary": "Update a season"
      }
    },
